out_to_console yes
update_interval 1.0
total_run_times 0
use_spacer none

TEXT

Up: ${uptime_short} | \
${if_match "${acpitemp}" > "0"}Temp:${acpitemp}C | ${endif}\
${if_match "${battery_percent}" != "0"}Batt: ${battery_short} | ${endif}\
RAM:$memperc% | \
CPU:${cpu}% | \
${if_match "${addr enp1s0}" != "No Address"}\
Eth: ${addr enp1s0} | \
Eth: ${downspeedf enp1s0} kB/s | \
${endif}\
${if_match "${addr wlp2s0}" != "No Address"}\
Wi: ${addr wlp2s0} | \
Wi: ${downspeedf wlp2s0} kB/s | \
${endif}\
        ${time %a %r %Z  %B %d, %Y}

